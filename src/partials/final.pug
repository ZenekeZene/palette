// FINAL PAGE
aside#finalPage.final.hidden
	header.header
		p#backButtonFinal.header__back
	.final-message.lives-out.hidden
		h2 Well done!
		h3 You have finished Palette with #[span 1530] points at...
		p
			span LEVEL
			span#contSuccesFinalPage 10
		a#shareLinkFinal.header__share(href="https://twitter.com/intent/tweet?text=I+have+finished+@PlayPalette+at+level+10+with+1530+points!!!+Can+you+make+it+better?+http://palette.ws") SHARE YOUR RECORD
	.final-message.game-end
		h2 Congratulations!
		h3 You have overcome all the levels of Palette with #[span 1530] points
		p
		a#shareLinkFinal.header__share(href="https://twitter.com/intent/tweet?text=I+have+overcome+all+the+levels+of+@PlayPalette+with+1530+points!!!+Can+you+make+it+better?+http://palette.ws") SHARE YOUR RECORD
	.final-donation Support us
	button.donate-button
	.confetti
		- var n = 0;
		while n < 13
			.confetti-piece
			- n++
